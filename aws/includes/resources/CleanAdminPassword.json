{
    "AWSTemplateFormatVersion": "2010-09-09",
    "Parameters": {
        "AdminPasswordFileId": { "Type": "String" },
        "BucketId": { "Type": "String" },
        "DeletePasswordLambda": { "Type": "String" },
        "DelayerParameter": { "Type": "String", "Default": "" }
    },
    "Resources": {
		"CleanAdminPassword": {
            "Type": "AWS::CloudFormation::CustomResource",
            "Version": "1.0",
            "Properties": {
                "ServiceToken": {
                    "Ref": "DeletePasswordLambda"
                },
                "bucket": {
                    "Ref": "BucketId"
                },
                "key": {
                    "Ref": "AdminPasswordFileId"
                }
            }
        }
    }
}