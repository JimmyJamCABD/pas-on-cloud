{
    "AWSTemplateFormatVersion": "2010-09-09",
    "Parameters": {
        "DeletePasswordLambda": { "Type": "String" },
        "BucketId": { "Type": "String" },
        "DRPasswordFileId": { "Type": "String" }
    },
    "Resources": {
		"CleanDRPassword": {
            "Type": "AWS::CloudFormation::CustomResource",
            "Version": "1.0",
            "Properties": {
                "ServiceToken": {
                    "Ref": "DeletePasswordLambda"
                },
                "bucket": {
                    "Ref": "BucketId"
                },
                "key": {
                    "Ref": "DRPasswordFileId"
                }
            }
        }
    }
}